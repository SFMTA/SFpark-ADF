<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.RateChangeManagerBundle</base-name>
      <var>rateChangeManagerBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.ButtonBundle</base-name>
      <var>buttonBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.ColumnBundle</base-name>
      <var>columnBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.CommonBundle</base-name>
      <var>commonBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.ErrorBundle</base-name>
      <var>errorBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.LabelBundle</base-name>
      <var>labelBundle</var>
    </resource-bundle>
  </application>
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <managed-bean>
    <managed-bean-name>globalBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.common.GlobalBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>navigationBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.common.NavigationBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>modificationBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.common.ModificationBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>deploymentBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.common.DeploymentBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>thresholdPropertiesBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.threshold.ThresholdPropertiesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>pickThresholdValuesBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.threshold.helper.PickThresholdValuesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>rateChangePropertiesBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.rateChange.RateChangePropertiesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>rateChangeUpdateBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.rateChange.RateChangeUpdateBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>rateChangeDeployBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.rateChange.RateChangeDeployBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>pickAreaTypeBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.rateChange.helper.PickAreaTypeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>pickCalendarBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.rateChange.helper.PickCalendarBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>pickMeterBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.rateChange.helper.PickMeterBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>pickPMDistrictsAndBlocksBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.rateChange.helper.PickPMDistrictsAndBlocksBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>modificationPropertiesBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.modification.ModificationPropertiesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>deploymentEditBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.deployment.DeploymentEditBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>deploymentPropertiesBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.deployment.DeploymentPropertiesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>deploymentDeleteBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.deployment.DeploymentDeleteBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>errorsBean</managed-bean-name>
    <managed-bean-class>sfpark.rateChange.manager.view.backing.common.ErrorsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- Listeners -->
  <lifecycle>
    <phase-listener id="nocache">sfpark.rateChange.manager.application.listener.CacheControlListener</phase-listener>
  </lifecycle>
</faces-config>
