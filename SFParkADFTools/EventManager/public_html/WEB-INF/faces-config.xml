<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <resource-bundle>
      <base-name>sfpark.event.manager.i18n.ApplicationBundle</base-name>
      <var>applicationBundle</var>
    </resource-bundle>
  </application>
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <managed-bean>
    <managed-bean-name>dialogBean</managed-bean-name>
    <managed-bean-class>sfpark.event.manager.view.backing.util.dialog.DialogBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>navigationBean</managed-bean-name>
    <managed-bean-class>sfpark.event.manager.view.backing.common.NavigationBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>globalBean</managed-bean-name>
    <managed-bean-class>sfpark.event.manager.view.backing.common.GlobalBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>errorsBean</managed-bean-name>
    <managed-bean-class>sfpark.event.manager.view.backing.common.ErrorsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>eventCalendarListBean</managed-bean-name>
    <managed-bean-class>sfpark.event.manager.view.backing.eventCalendar.EventCalendarListBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>eventCalendarPropertiesBean</managed-bean-name>
    <managed-bean-class>sfpark.event.manager.view.backing.eventCalendar.EventCalendarPropertiesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>addEventCalendarDateBean</managed-bean-name>
    <managed-bean-class>sfpark.event.manager.view.backing.eventCalendar.AddEventCalendarDateBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>editEventCalendarNameBean</managed-bean-name>
    <managed-bean-class>sfpark.event.manager.view.backing.eventCalendar.EditEventCalendarNameBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- Listeners -->
  <lifecycle>
    <phase-listener id="nocache">sfpark.event.manager.application.listener.CacheControlListener</phase-listener>
  </lifecycle>
</faces-config>