    <af:panelGroupLayout id="parkingSpaceInventoryPanel" layout="horizontal"
                         valign="top" partialTriggers="validateAndSaveButton">
      <f:facet name="separator">
        <af:spacer id="parkingSpaceInventoryPanelSpacer" width="50"/>
      </f:facet>
      <af:panelFormLayout id="lhsPSIFormPanel">
        <af:panelLabelAndMessage id="postIDPLM" for="postIDIT"
                                 label="#{labelBundle.label_post_id}">
          <af:inputText id="postIDIT" required="true"
                        maximumLength="#{meterSpaceManagementBean.postIDITMaximumLength}"
                        columns="#{meterSpaceManagementBean.postIDITColumns}"
                        readOnly="#{meterSpaceManagementBean.readOnly}"
                        label="#{meterSpaceManagementBean.postIDITLabel}"
                        value="#{meterSpaceManagementBean.postIDITValue}"
                        autoSubmit="true"
                        validator="#{meterSpaceManagementBean.postIDValidator}"
                        partialTriggers="activeMeterStatusSOC"/>
        </af:panelLabelAndMessage>
        <af:spacer id="postIDSpacer" height="10"/>
        <af:panelLabelAndMessage id="streetTypePLM"
                                 rendered="#{meterSpaceManagementBean.renderStreetTypePLM}"
                                 for="streetTypeSOR"
                                 label="#{labelBundle.label_street_type}"
                                 labelStyle="vertical-align: top;">
          <af:selectOneRadio id="streetTypeSOR" layout="horizontal"
                             value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.onOffStreetType}"
                             autoSubmit="true"
                             valueChangeListener="#{meterSpaceManagementBean.streetTypeValueChangeHandler}"
                             binding="#{meterSpaceManagementBean.streetTypeSOR}">
            <f:selectItems id="streetTypeSORSI"
                           value="#{meterSpaceManagementBean.listStreetType}"/>
          </af:selectOneRadio>
          <af:selectOneChoice id="ospIDSOC" partialTriggers="streetTypeSOR"
                              disabled="#{meterSpaceManagementBean.disableOSPIDSOC}"
                              label="#{labelBundle.label_osp_id}"
                              value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.OSPID}"
                              autoSubmit="true"
                              valueChangeListener="#{meterSpaceManagementBean.ospIDValueChangeHandler}">
            <f:selectItems id="ospIDSOCSI"
                           value="#{meterSpaceManagementBean.listOSPInventory}"/>
          </af:selectOneChoice>
        </af:panelLabelAndMessage>
        <af:selectOneChoice id="jurisdictionSOC"
                            rendered="#{meterSpaceManagementBean.renderStreetTypePLM}"
                            label="#{labelBundle.label_jurisdiction}"
                            value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.jurisdiction}"
                            autoSubmit="true">
          <f:selectItems id="jurisdictionSOCSI"
                         value="#{meterSpaceManagementBean.listJurisdiction}"/>
        </af:selectOneChoice>
        <af:spacer id="streetTypePLMSpacer" height="10"
                   rendered="#{meterSpaceManagementBean.renderStreetTypePLM}"/>
        <af:selectOneChoice id="sensorStatusSOC"
                            readOnly="#{meterSpaceManagementBean.readOnly}"
                            label="#{labelBundle.label_sensor_status}"
                            value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.sensorFlag}"
                            autoSubmit="true">
          <f:selectItems id="sensorStatusSOCSI"
                         value="#{meterSpaceManagementBean.listSensorStatus}"/>
        </af:selectOneChoice>
        <af:spacer id="sensorStatusSOCSpacer" height="10"/>
        <af:selectOneChoice id="capColorSOC"
                            partialTriggers="deleteParkingSpaceButton undeleteParkingSpaceButton"
                            disabled="#{meterSpaceManagementBean.disableCapColorSOC}"
                            readOnly="#{meterSpaceManagementBean.readOnly}"
                            label="#{labelBundle.label_cap_color}"
                            value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.capColor}"
                            autoSubmit="true">
          <f:selectItems id="capColorSOCSI"
                         value="#{meterSpaceManagementBean.listCapColorSOC}"/>
        </af:selectOneChoice>
        <af:spacer id="capColorSOCSpacer" height="10"/>
        <af:selectOneChoice id="activeMeterStatusSOC"
                            partialTriggers="deleteParkingSpaceButton undeleteParkingSpaceButton"
                            disabled="#{meterSpaceManagementBean.disableActiveMeterStatusSOC}"
                            readOnly="#{meterSpaceManagementBean.readOnly}"
                            label="#{labelBundle.label_active_meter_status}"
                            value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.activeMeterFlag}"
                            autoSubmit="true"
                            valueChangeListener="#{meterSpaceManagementBean.activeMeterStatusValueChangeHandler}">
          <f:selectItems id="activeMeterStatusSOCSI"
                         value="#{meterSpaceManagementBean.listActiveMeterStatus}"/>
        </af:selectOneChoice>
        <af:spacer id="activeMeterStatusSOCSpacer" height="10"/>
        <af:selectOneChoice id="reasonCodeSOC"
                            readOnly="#{meterSpaceManagementBean.readOnly}"
                            label="#{labelBundle.label_reason_code}"
                            value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.reasonCode}"
                            autoSubmit="true">
          <f:selectItems id="reasonCodeSOCSI"
                         value="#{meterSpaceManagementBean.listReasonCode}"/>
        </af:selectOneChoice>
        <af:spacer id="reasonCodeSOCSpacer" height="10"/>
        <af:inputText id="legislationRefIT" columns="11" maximumLength="10"
                      readOnly="#{meterSpaceManagementBean.readOnly}"
                      label="#{labelBundle.label_legislation_reference}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.legislationReference}"
                      autoSubmit="true"/>
        <af:inputDate id="legislationDateID" columns="11"
                      readOnly="#{meterSpaceManagementBean.readOnly}"
                      label="#{labelBundle.label_legislation_date}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.legislationDate}"
                      autoSubmit="true">
          <af:convertDateTime type="date" timeZone="America/Los_Angeles"/>
        </af:inputDate>
        <af:spacer id="legislationSpacer" height="10"/>
        <af:inputText id="workOrderIT" columns="8" maximumLength="7"
                      readOnly="#{meterSpaceManagementBean.readOnly}"
                      label="#{labelBundle.label_work_order}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.workOrder}"
                      autoSubmit="true"/>
        <af:spacer id="workOrderITSpacer" height="10"/>
      </af:panelFormLayout>
      <af:panelFormLayout id="centerPSIFormPanel"
                          styleClass="centerPanelFormLayout">
        <af:commandButton id="meterModelsButton"
                          rendered="#{meterSpaceManagementBean.renderButtons}"
                          disabled="#{meterSpaceManagementBean.disableMeterModelsButton}"
                          inlineStyle="width: 150px;"
                          text="#{buttonBundle.button_change_meter_model}"
                          actionListener="#{meterSpaceManagementBean.editButtonHandler}"
                          partialTriggers="activeMeterStatusSOC"
                          immediate="true"/>
        <af:inputText id="smartMeterIT" readOnly="true"
                      label="#{labelBundle.label_smart_meter}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.meterDetails.displaySmartMeterFlag}"
                      binding="#{meterSpaceManagementBean.smartMeterIT}"/>
        <af:inputText id="meterTypeIT" readOnly="true"
                      label="#{labelBundle.label_meter_type}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.meterDetails.displayMeterType}"
                      binding="#{meterSpaceManagementBean.meterTypeIT}"/>
        <af:inputText id="msPayStationIDIT" maximumLength="9" columns="10"
                      required="true" showRequired="false"
                      disabled="#{meterSpaceManagementBean.disableMSPayStationIDIT}"
                      readOnly="#{meterSpaceManagementBean.readOnly}"
                      label="#{labelBundle.label_ms_pay_station_id}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.MSPayStationID}"
                      binding="#{meterSpaceManagementBean.msPayStationIDIT}">
          <af:validateRegExp pattern="#{globalBean.postIDPattern}"
                             messageDetailNoMatch="#{assetManagerBundle.info_ms_pay_station_id_validator}"/>
        </af:inputText>
        <af:inputText id="msNumberIT" maximumLength="2" columns="3"
                      required="true" showRequired="false"
                      disabled="#{meterSpaceManagementBean.disableMSNumberIT}"
                      readOnly="#{meterSpaceManagementBean.readOnly}"
                      label="#{labelBundle.label_ms_number}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.MSSpaceNum}"
                      binding="#{meterSpaceManagementBean.msNumberIT}">
          <af:validateLongRange minimum="1" maximum="99"/>
        </af:inputText>
        <af:spacer id="msNumberITSpacer" height="10"/>
        <af:commandButton id="cnnIDButton"
                          rendered="#{meterSpaceManagementBean.renderButtons}"
                          disabled="#{meterSpaceManagementBean.disableCNNIDButton}"
                          inlineStyle="width: 100px;"
                          text="#{buttonBundle.button_change_cnn_id}"
                          actionListener="#{meterSpaceManagementBean.editButtonHandler}"
                          partialTriggers="activeMeterStatusSOC streetTypeSOR"
                          immediate="true"/>
        <af:inputText id="cnnIDIT" required="true" readOnly="true"
                      label="#{labelBundle.label_cnn_id}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.CNNID}"
                      binding="#{meterSpaceManagementBean.cnnIDIT}"/>
        <af:spacer id="cnnIDITSpacer" height="10"/>
        <af:inputText id="streetNumberIT" columns="6" required="true"
                      showRequired="false"
                      partialTriggers="validateAndSaveButton streetTypeSOR"
                      disabled="#{meterSpaceManagementBean.disableStreetNumberIT}"
                      readOnly="#{meterSpaceManagementBean.readOnly}"
                      label="#{labelBundle.label_street_number}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.streetNum}">
          <af:validateLongRange minimum="0"/>
        </af:inputText>
        <af:spacer id="streetNumberITSpacer" height="10"/>
        <af:inputText id="commentsIT" columns="40" maximumLength="50" rows="5"
                      readOnly="#{meterSpaceManagementBean.readOnly}"
                      label="#{labelBundle.label_comments}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.comments}"
                      autoSubmit="true"/>
        <af:spacer id="commentsITSpacer" height="10"/>
      </af:panelFormLayout>
      <af:panelFormLayout id="rhsPSIFormPanel">
        <af:panelLabelAndMessage id="blockfaceIDPLM" for="blockfaceIDIT"
                                 label="#{labelBundle.label_blockface_id}">
          <af:inputText id="blockfaceIDIT" columns="2" required="true"
                        partialTriggers="activeMeterStatusSOC"
                        readOnly="#{meterSpaceManagementBean.readOnly}"
                        label="#{meterSpaceManagementBean.blockfaceIDITLabel}"
                        value="#{meterSpaceManagementBean.blockfaceIDITValue}">
            <af:validateRegExp pattern="#{globalBean.singleDigitPattern}"
                               messageDetailNoMatch="#{commonBundle.info_only_digits_allowed}"/>
          </af:inputText>
        </af:panelLabelAndMessage>
        <af:inputText id="streetIDIT" readOnly="true"
                      partialTriggers="validateAndSaveButton"
                      label="#{labelBundle.label_street_id}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.streetID}"/>
        <af:inputText id="streetNameIT" readOnly="true"
                      partialTriggers="validateAndSaveButton"
                      label="#{labelBundle.label_street_name}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.streetName}"/>
        <af:inputText id="orientationIT" readOnly="true"
                      partialTriggers="validateAndSaveButton"
                      label="#{labelBundle.label_orientation}"
                      value="#{meterSpaceManagementBean.currentParkingSpaceInventoryDTO.orientation}"/>
        <af:spacer id="blockfaceIDITSpacer" height="10"/>
        <af:selectOneChoice id="parityDigitPositionSOC"
                            partialTriggers="streetTypeSOR"
                            contentStyle="width: 40px;"
                            readOnly="#{meterSpaceManagementBean.readOnly}"
