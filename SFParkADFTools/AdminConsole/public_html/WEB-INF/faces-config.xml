<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.AdminConsoleBundle</base-name>
      <var>adminConsoleBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.ButtonBundle</base-name>
      <var>buttonBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.ColumnBundle</base-name>
      <var>columnBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.CommonBundle</base-name>
      <var>commonBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.ErrorBundle</base-name>
      <var>errorBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>sfpark.adf.tools.translation.LabelBundle</base-name>
      <var>labelBundle</var>
    </resource-bundle>
  </application>
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <managed-bean>
    <managed-bean-name>globalBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.common.GlobalBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>navigationBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.common.NavigationBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>allowedValuesListBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.allowedValues.AllowedValuesListBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>allowedValuesPropertiesBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.allowedValues.AllowedValuesPropertiesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>timebandListBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.timeband.TimebandListBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>pickTimebandTypeBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.timeband.helper.PickTimebandTypeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>pickTimebandValuesBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.timeband.helper.add.PickTimebandValuesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>timebandAddBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.timeband.TimebandAddBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>timebandDeleteBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.timeband.TimebandDeleteBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>errorsBean</managed-bean-name>
    <managed-bean-class>sfpark.admin.console.view.backing.common.ErrorsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- Listeners -->
  <lifecycle>
    <phase-listener id="nocache">sfpark.admin.console.application.listener.CacheControlListener</phase-listener>
  </lifecycle>
</faces-config>