package sfpark.adf.tools.model.helper.dO;

import sfpark.adf.tools.model.data.dO.pcoBeats.PCOBeatsDO;

public class PCOBeatsDOStatus implements DOStatusInterface {

    private boolean exists;
    private PCOBeatsDO DO;

    public PCOBeatsDOStatus(PCOBeatsDO DO) {
        super();

        this.exists = (DO != null);
        this.DO = DO;
    }

    public boolean existsDO() {
        return exists;
    }

    public PCOBeatsDO getDO() {
        return DO;
    }
}
